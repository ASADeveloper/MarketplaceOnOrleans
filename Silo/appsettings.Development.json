{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Orleans.Runtime": "Warning",
      "Orleans.Transactions": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "SellerViewPostgres": true,
    "StreamReplication": false,
    "OrleansTransactions": true,
