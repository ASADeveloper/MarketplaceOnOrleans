    "NumShipmentActors": 100,
    "UseDashboard": true,
    "UseSwagger": true,
    "RedisReplication": false,
    "RedisPrimaryConnectionString": "localhost:6379",
    "RedisSecondaryConnectionString": "localhost:6380"
  }
}

